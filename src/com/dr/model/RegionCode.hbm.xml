<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.dr.model.RegionCode" table="regioncode">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="c1" type="java.lang.String"><column name="c1" /></property>
		<property name="c2" type="java.lang.String"><column name="c2" /></property>
		<property name="c3" type="java.lang.String"><column name="c3" /></property>
		<property name="code" type="java.lang.String"><column name="code" /></property>				
		
	</class>
</hibernate-mapping>
